<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="theme_new" parent="Theme.AppCompat.Light.NoActionBar" >
        <item name="colorPrimary">@color/primary_material_light_1</item>
        <item name="android:colorBackground">@color/background_material_light</item>
        <item name="android:colorForeground">@color/foreground_material_light</item>
    </style>

    <declare-styleable name="ExpandleableView">
        <attr name="stroke_with" format="dimension"></attr>
        <attr name="stroke_color" format="color"></attr>
        <attr name="round_raduis" format="dimension"></attr>
        <attr name="back_color" format="color"></attr>
        <attr name="button_size" format="dimension"></attr>
        <attr name="button_color" format="color"></attr>
        <attr name="button_stroke_size" format="dimension"></attr>
    </declare-styleable>

</resources>
